# API Configuration
# Replace with your actual backend API URL
# Example: http://localhost:5003 or http://197.243.0.108:5003
# IMPORTANT: Make sure your backend server is running at this URL
NEXT_PUBLIC_API_BASE_URL=http://localhost:5003
NEXT_PUBLIC_API_VERSION=v1

# App Configuration
NEXT_PUBLIC_APP_NAME=Car Rental
NEXT_PUBLIC_APP_VERSION=1.0.0

# Authentication
NEXT_PUBLIC_TOKEN_KEY=car_rental_token
NEXT_PUBLIC_REFRESH_TOKEN_KEY=car_rental_refresh_token
NEXT_PUBLIC_USER_DATA_KEY=car_rental_user_data

# Environment
NODE_ENV=development

# Google Maps API Key
# Get your API key from: https://console.cloud.google.com/google/maps-apis
# Make sure to enable: Maps JavaScript API, Places API, Geocoding API
NEXT_PUBLIC_GOOGLE_MAPS_SECRET_KEY=your_google_maps_api_key_here